<div id="<%= dom_id client %>">
  <p>
    <strong>Name:</strong>
    <%= client.name %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= client.phone %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= client.email %>
  </p>

  <% client.calls.each do |call| %>
    <p>Tipo: <%=call.reason.name%> - Operador: <%=call.user.email%> - Fecha: <%=call.created_at%></p>
  <% end %>

</div>
